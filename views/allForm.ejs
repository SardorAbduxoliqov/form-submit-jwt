<!DOCTYPE html>
<html>
  <head>
    <style>
      table {
        font-family: arial, sans-serif;
        border-collapse: collapse;
        width: 100%;
      }

      td,
      th {
        border: 1px solid #dddddd;
        text-align: left;
        padding: 8px;
      }

      tr:nth-child(even) {
        background-color: #dddddd;
      }
    </style>
  </head>
  <body>
    <h2>All Forms</h2>

    <table>
      <tr>
        <th>Name</th>
        <th>Course</th>
        <th>Phone</th>
      </tr>
      <% if (forms.length > 0) { %> <% for (let form of forms) { %>

      <tr>
        <td><%= form.name %></td>
        <td><%= form.course %></td>
        <td><%= form.phoneNumber %></td>
      </tr>
      <% } %> <% } else { %>
      <h1>No Products Found!</h1>
      <% } %>
    </table>
  </body>
</html>
